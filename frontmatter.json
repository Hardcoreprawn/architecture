{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "NavBarTitle",
          "name": "navbartitle",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Lead",
          "name": "lead",
          "type": "string"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Published",
          "name": "published",
          "type": "datetime"
        },
        {
          "title": "Image",
          "name": "image",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "other",
  "frontMatter.content.publicFolder": "[[workspace]]/input/media",
  "frontMatter.content.pageFolders": [
    {
      "title": "Posts",
      "path": "[[workspace]]/input/posts"
    },
    {
      "title": "Architecture",
      "path": "[[workspace]]/input/posts/architecture"
    },
    {
      "title": "Patterns-and-antipatterns",
      "path": "[[workspace]]/input/posts/patterns-and-antipatterns"
    },
    {
      "title": "Microservices",
      "path": "[[workspace]]/input/posts/patterns-and-antipatterns/microservices"
    },
    {
      "title": "Requirements",
      "path": "[[workspace]]/input/posts/requirements"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "Application",
    "Architectural Principles",
    "Architecture",
    "Business",
    "Data",
    "Data",
    "Microservices",
    "Non-Functional Requirements",
    "Patterns",
    "Requirements",
    "Security",
    "Technical",
    "Technology"
  ],
  "frontMatter.taxonomy.categories": [
    "Pattern",
    "Principles",
    "Requirements"
  ]
}