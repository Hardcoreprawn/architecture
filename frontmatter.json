{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "Title",
          "type": "string"
        },
        {
          "title": "Lead",
          "name": "Lead",
          "type": "string"
        },
        {
          "title": "Tags",
          "name": "Tags",
          "type": "tags"
        },
        {
          "title": "Draft",
          "name": "Draft",
          "type": "boolean"
        },
        {
          "title": "Published",
          "name": "Published",
          "type": "string"
        }
        }
      ]
    }
  ],
  "frontMatter.framework.id": "other",
