{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "Title",
          "type": "string"
        },
        {
          "title": "Lead",
          "name": "Lead",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "Description",
          "type": "string"
        },
        {
          "title": "Tags",
          "name": "Tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "Categories",
          "type": "categories"
        },
        {
          "title": "Draft",
          "name": "Draft",
          "type": "boolean"
        },
        {
          "title": "Published",
          "name": "Published",
          "type": "datetime"
        },
        {
          "title": "Category",
          "name": "Category",
          "type": "categories"
        },
        {
          "title": "Author",
          "name": "Author",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "other",
  "frontMatter.content.draftField": {
    "name": "Draft",
    "type": "boolean"
  },
  "frontMatter.content.publicFolder": "[[workspace]]/input/media",
  "frontMatter.content.pageFolders": [
    {
      "title": "Principles",
      "path": "[[workspace]]/input/posts/principles"
    },
    {
      "title": "Patterns-and-antipatterns",
      "path": "[[workspace]]/input/posts/patterns-and-antipatterns"
    },
    {
      "title": "Requirements",
      "path": "[[workspace]]/input/posts/requirements"
    },
    {
      "title": "Architectural-Decision-Records",
      "path": "[[workspace]]/input/posts/architectural-decision-records"
    },
    {
      "title": "Solutions",
      "path": "[[workspace]]/input/posts/solutions"
    }
  ],
  "frontMatter.taxonomy.seoDescriptionField": "Description",
  "frontMatter.taxonomy.seoTitleField": "Title",
  "frontMatter.taxonomy.tags": [
    "Application Principles",
    "Architectural Principles",
    "Architecture",
    "Business",
    "Business Principles",
    "Data",
    "Data Principles",
    "Microservices",
    "NFRs",
    "Non-Functional Requirements",
    "Patterns",
    "Process",
    "Requirements",
    "Security",
    "Solution",
    "Technical Principles"
  ],
  "frontMatter.taxonomy.categories": [
    "Patterns",
    "Principles",
    "Requirements",
    "Solution"
  ],
  "frontMatter.dashboard.openOnStart": true
}